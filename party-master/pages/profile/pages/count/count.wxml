<!--pages/profile/pages/count/count.wxml-->
<view class="index">
  <view class="index-bd">
    <view class="kind-list">
      <block wx:for-items="{{list}}" wx:key="{{item.id}}">
        <view class="kind-list-item-bd kind-list-item-bd-show">
          <view class="navigator-box navigator-box-show">
           <navigator url="" class="navigator">
            <view class="navigator-text">{{item.name}}:</view>+{{item.money}}
           </navigator> 
          </view>
        </view>
      </block>
    </view>
  </view>
</view>